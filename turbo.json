{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "build": {
      "env": [
        "APP_NAME",
        "API_URL",
        "SESSION_SECRET",
        "GOOGLE_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID"
      ],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
